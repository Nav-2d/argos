{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["build"]
    },
    "setup": {
      "cache": false
    }
  },
  "globalDependencies": ["tsconfig.json", ".swcrc"]
}
