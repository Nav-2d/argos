{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript"
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "target": "es2022"
  },
  "module": {
    "type": "es6",
    "resolveFully": true
  },
  "sourceMaps": false
}
